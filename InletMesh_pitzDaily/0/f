FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      v2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletFace
    {
        type            empty;
    }

    inlet
    {
        type            zeroGradient;
    }

    inletWalls
    {
        type            fWallFunction;
        value           uniform 0;
    }

    outletInlet
    {
        type            zeroGradient;
    }
}


