FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "5";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    sourceFaceleft
    {
        type            calculated;
        value           uniform 0;
    }
    destinationFaceleft
    {
        type            calculated;
        value           uniform 0;
    }
    channelWallsleft
    {
        type            calculated;
        value           uniform 0;
    }
    leftRightWallleft
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
}


